<div class="tasks-form">
    <h2 class="form-title">Tasks</h2>
    <input id="add-new-task" data-listId="{{listId}}" class="add-new-task" type="text" placeholder="Add new task" />
    <ul class="tasks-list">
      {{#each tasks}}
        {{> templates_task_task-element}}
      {{/each}}
    </ul>
</div>